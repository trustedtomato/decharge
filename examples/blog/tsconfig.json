{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "preserveWatchOutput": true,
    "module": "ESNext",
    "target": "ES2017",
    "moduleResolution": "node",
    "outDir": "/dev/null",
    "rootDir": "src",
    "allowJs": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/routes/**/*"]
}